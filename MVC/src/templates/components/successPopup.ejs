<% var success = typeof success !== 'undefined' ? success : null; %>
<% if (success && success.length) {  %>
<div class="bg-green-100 border border-green-400 text-green-700 px-4 py-3 rounded absolute left-1/2 transform -translate-x-1/2 top-2 mb-4 animate-fade-out"
	role="alert">
	<strong class="font-bold">Success:</strong>
	<ul class="mt-2 list-disc list-inside">
		<% success.forEach((element) => { %>
			<li> <%= element %> </li>
		<% }); %>
	</ul>
</div>
<% } %>